<div class="form">
  <h1 class="title">Registro</h1>
  <form id="loginForm" method="post">
    <label for="email">Correo electrónico:</label>
    <div class="input-group">
      <div class="svg-input"><i class="fas fa-envelope" style="line-height:inherit;"></i></div>
      <input type="email" id="email" autocomplete="email" placeholder="Ingrese correo" />
    </div>
    <br />
    <label for="pass">Contraseña:</label>
    <div class="input-group">
      <div class="svg-input"><i class="fas fa-lock" style="line-height:inherit;"></i></div>
      <input type="password" id="pass" autocomplete="current-password" placeholder="Ingrese contraseña" />
    </div>
    <br />
    <button type="submit">Enviar</button>
  </form>
</div>
<script type="module" src="ingreso.js"></script>
